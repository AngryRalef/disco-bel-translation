                                                {
                                                  "id": 24,
                                                  "title": "Inland Empire: \"Her?\"",
                                                  "articyId": "0x0100005100001E0B",
                                                  "text": "Her?",
                                                  "links": [
                                                    {
                                                      "id": 71,
                                                      "title": "Volition: \"No one, don't ask about her.\"",
                                                      "articyId": "0x0100005100001E15",
                                                      "text": "No one, don't ask about her.",
                                                      "links": [
                                                        {
                                                          "id": 40,
                                                          "title": "You: \"Wait, get who back?\"",
